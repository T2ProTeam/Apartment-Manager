<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:people="clr-namespace:ViewModel.People;assembly=ViewModel"
                    xmlns:room="clr-namespace:ViewModel.Room;assembly=ViewModel"
                    xmlns:roomview="clr-namespace:AM.UI.View.Rooms"
                    xmlns:view="clr-namespace:AM.UI.View"
                    xmlns:vm ="clr-namespace:AM.UI.ViewModelUI"
                    xmlns:vp="clr-namespace:AM.UI.View.People"
                    xmlns:vr="clr-namespace:AM.UI.View.Rooms"
                    xmlns:vh="clr-namespace:UI.View.Homes"
                    >
    
    <DataTemplate DataType="{x:Type vm:RoomAddUI}">
        <vr:RoomAdd/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <vh:Home/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:NavigationVM}">
        <vh:Home/>
    </DataTemplate>

</ResourceDictionary>