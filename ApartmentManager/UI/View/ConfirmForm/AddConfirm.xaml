<UserControl x:Class="UI.View.ConfirmForm.AddConfirm"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UI.View.ConfirmForm"
             mc:Ignorable="d" 
             d:DesignHeight="630" d:DesignWidth="730">
    <Grid>
        <Button Style="{StaticResource ConfirmButton}" Content="Confirm" HorizontalAlignment="Left" Margin="106,207,0,0" VerticalAlignment="Top" Height="55"/>
        <Button Style="{StaticResource ConfirmButton}" Content="Cancel" HorizontalAlignment="Left" Margin="418,239,0,0" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
